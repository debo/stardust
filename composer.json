{
  "name": "stardust/stardust",
  "description": "Stardust is what the application of your dreams is made of",
  "type": "project",
  "license": "MIT",
  "keywords": ["framework"],
  "homepage": "http://stardust.com/",
  "authors": [
    {
      "name": "Marco \"Debo\" De Bortoli",
      "email": "marco.debo.debortoli@gmail.com",
      "homepage": "https://debo.io",
      "role": "Developer"
    }
  ],
  "minimum-stability": "dev",
  "prefer-stable": true,
  "autoload": {
    "psr-4": {
      "Stardust\\": "src"
    }
  },
  "config": {
    "bin-dir": "bin",
    "allow-plugins": {
      "nodejs-php-fallback/nodejs-php-fallback": true
    }
  },
  "require": {
    "php": "^8.0",
    "symfony/http-foundation": "^6.3",
    "symfony/routing": "^6.3",
    "symfony/http-kernel": "^6.3",
    "symfony/event-dispatcher": "^6.3",
    "symfony/dependency-injection": "^6.3",
    "symfony/proxy-manager-bridge": "^6.3",
    "symfony/config": "^6.3",
    "twig/twig": "^3.7",
    "monolog/monolog": "^3.4",
    "pug-php/pug": "^3.5"
  },
  "require-dev": {
    "roave/security-advisories": "dev-latest",
    "behat/behat": "^3.13",
    "phpspec/phpspec": "^7.4",
    "phpunit/phpunit": "^10.4",
    "sensiolabs/behat-page-object-extension": "^2.3",
    "squizlabs/php_codesniffer": "^3.7",
    "pdepend/pdepend": "^2.15",
    "phpmd/phpmd": "^2.14",
    "mikey179/vfsstream": "^1.6",
    "phpstan/phpstan": "^1.10"
  }
}
